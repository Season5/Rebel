<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.onikah.greenday.Fragment2">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/meet"
                android:id="@+id/meet"
                android:padding="20dp"
                android:textAlignment="center"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="143dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Morning Session"
                android:textSize="30dp"
                android:id="@+id/morn"
                android:textColor="#000"
                android:layout_marginTop="101dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView"
                android:src="@drawable/threecircle"
                android:layout_alignTop="@+id/morn"
                android:layout_toLeftOf="@+id/morn"
                android:layout_toStartOf="@+id/morn" />


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/lunch"
                android:id="@+id/lunch"
                android:padding="20dp"
                android:textAlignment="center"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="370dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Food/Lunch"
                android:textSize="30dp"
                android:id="@+id/food"
                android:textColor="#000"
                android:layout_marginTop="340dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView2"
                android:src="@drawable/ic_food"
                android:layout_alignTop="@+id/food"
                android:layout_toLeftOf="@+id/food"
                android:layout_toStartOf="@+id/food" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/noon"
                android:id="@+id/textView"
                android:padding="20dp"
                android:textAlignment="center"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="540dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Afternoon Session"
                android:textSize="30dp"
                android:id="@+id/noon"
                android:textColor="#000"
                android:layout_marginTop="520dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/another"
                android:src="@drawable/info"
                android:layout_alignTop="@+id/noon"
                android:layout_toLeftOf="@+id/noon"
                android:layout_toStartOf="@+id/noon" />


        </RelativeLayout>

    </ScrollView>
</FrameLayout>
